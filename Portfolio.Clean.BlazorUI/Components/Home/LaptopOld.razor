
<div class="laptop-container" style="
    --laptop-position-top: @PositionTop;
    --laptop-position-left: @PositionLeft;
    --laptop-container-opacity: @Opacity;
    --laptop-container-width: @ContainerWidth;
    --laptop-container-height: @ContainerHeight;
    --laptop-width: @LaptopWidth;
    --laptop-height: @LaptopHeight;
    --laptop-z-index: @ZIndex;
">
    <img src="/images/Laptop JBT.DEV@1-Purple-2048x2048.png" />
    <div class="laptop-text">
        <span class="txt-type" data-wait="3000"
                data-words=@TypingWords>
        </span>
    </div>
</div>




